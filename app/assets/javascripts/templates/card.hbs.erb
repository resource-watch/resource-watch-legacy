<div class="chart"></div>
<div class="detail">
  <h3><a href="/explore/{{data.slug}}">{{data.name}}</a></h3>
  <p>{{data.source}}</p>

  <div class="actions">
    <!-- <button class="action -plain -wicon">
      <svg class="icon icon-download">
        <use xlink:href="#icon-download"></use>
      </svg>
      <span>Download</span> -->
    </button>

    <button class="action -plain -pink tooltip-cs"><%= I18n.t('map') %></button>

    <button class="action -plain -gree tooltip-cs"><%= I18n.t('CSV') %></button>

    <button class="action -plain -wicon favorite tooltip-cs">
      <svg class="icon icon-star">
        <use xlink:href="#icon-star"></use>
      </svg>
      <span>0</span>
    </button>
  </div>
</div>
{{#if state.actions}}
  <div class="actions">
    {{#if data.layer}}
      <button class="action -gray js-toggle-layer"><%= I18n.t('add_to_map') %></button>
    {{else}}
      <button class="action -disabled"><%= I18n.t('not_displayable') %></button>
    {{/if}}
  </div>
{{/if}}
